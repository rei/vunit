    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "chokidar": "^2.0.4",
    "cross-env": "^5.0.5",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.7",
    "expect": "^21.2.1",
    "glob": "^7.1.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^12.1.0",
    "jsdom-global": "^3.0.2",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.9.4",
    "npx": "^10.2.0",
    "nyc": "^12.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue": "^2.4.4",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-dev-server": "^2.9.1",
