{
  "name": "@rei/vunit",
  "description": "Write unit tests against your Vue components using mocha and vue-test-utils",
  "version": "2.2.0",
  "author": "Alex Perkins <peripateticus.gh@gmail.com>",
  "repository": "https://github.com/rei/vunit",
  "license": "MIT",
  "main": "./index.js",
  "scripts": {
    "test": "mocha --timeout 8000"
  },
  "bin": {
    "vunit": "./index.js"
  },
  "engines": {
    "node": ">8.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "chokidar": "^3.3.1",
    "cross-env": "^5.0.5",
    "cross-spawn": "^6.0.5",
    "css-loader": "^3.4.2",
    "expect": "^25.1.0",
    "glob": "^7.1.6",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.13.1",
    "nyc": "^15.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-vue": "^6.1.2",
    "proxyquire": "^2.1.3",
    "webpack": "^4.42.0"
  }
}
